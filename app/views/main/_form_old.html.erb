  <div class="form-group datetime-group">
    <img src="/assets/header-image-login.png" class="img-responsive " style="margin: 5% auto" />
  </div>
  <div class="form-group datetime-group">
    <%= label_tag(:date,"Fecha",:class => "col-sm-5 control-label")%>
    <div class="col-sm-6">
     <%= text_field_tag(:date, @date  , :class => 'datepicker form-control') %>
    </div>
  </div>
  <div class="form-group datetime-group">
    <%= label_tag(:time,"Hora",:class => "col-sm-5 control-label")%>
    <div class="col-sm-6">
     <%= text_field_tag(:time, @time ,:class => "timepicker form-control") %>

    </div>
